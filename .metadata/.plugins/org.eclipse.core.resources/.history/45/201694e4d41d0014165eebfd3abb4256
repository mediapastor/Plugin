package com.tdgc.cocos2dx.popup.creator.model.basic;

public class Point {

	public Point(float pX, float pY) {
		this.mX = pX;
		this.mY = pY;
	}
	
	@Override
	public String toString() {
		return "CCPoint(" + mX + ", " + mY + ")";
	}
	
	public float getX() {
		return this.mX;
	}
	
	public float getY() {
		return this.mY;
	}
	
	private float mX, mY;
	
}
