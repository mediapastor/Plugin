package com.tvd.gameview.plugin.model;

public class ViewModel {
	
	public ViewModel(String name, boolean isDone) {
		this.mName = name;
		this.mIsDone = isDone;
	}
	
	public String getName() {
		return mName;
	}
	
	public boolean isDone() {
		return mIsDone;
	}

	private String mName;
	private boolean mIsDone;
}
