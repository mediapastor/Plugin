<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<view class-name="ProfilePopup" prefix="popup_profile" type="popup" exitable="true" 
		super="PopupLayer" background-name="default" size="260.0, 350.0"
		template-name="Common extend table"
		exported="false"
		comment=""
		xmlns="http://www.tvd.com/tools"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.tvd.com/tools ../xsd/view_2_0.xsd">
	<position-name value="profile_popup" />
	<position value="default" />
	<define-path value="/Volumes/DATA/Working/TicTacToe/client/caroonline/CaroOnline/Classes/Other/Utils/Defines" />
	<parameters-path value="/Volumes/DATA/Working/TicTacToe/client/caroonline/CaroOnline/Classes/Other/Utils/GameParams" />
	<images-inputpath value="/Users/dungtavan/Downloads/Skype/Profile/profile_popup" />
	<images-path value="/Volumes/DATA/Working/TicTacToe/client/caroonline/CaroOnline/Resources/iPadhd" />
	<xibcontainer-path value="/Volumes/DATA/Working/TicTacToe/client/caroonline/CaroOnline/Resources/screenshots/iPadhd" used="true" />
	<screencontainer-path value="/Volumes/DATA/Working/TicTacToe/client/caroonline/CaroOnline/Wireframing/WireframingScreen/iPadhd" />
	<androidcontainer-path value="/Volumes/DATA/Working/TicTacToe/client/caroonline/CaroOnline/proj.android/res/layout/iPadhd" />
	<class-path value="/Volumes/DATA/Working/TicTacToe/client/caroonline/CaroOnline/Classes/View/UI/Popup" />
	<background-image id="profile_popup_background_profile_bg" 
		location-in-interfacebuilder="10.0, 10.0"
		width="260.0" height="350.0"
		phony-path="profile_popup/background/profile_bg.png"  />
	<exit-image position="0.0, 0.0" location-in-interfacebuilder="100.0, 100.0" />

	<sprites array="false" comment="create group of sprites">
		<sprite visible="true" comment="">
			<position-name value="profile_avatar_default" />
			<anchorpoint value="0.5, 0.5" />
			<position value="50.0, 50.0" />
			<z-index value="0" />
			<image id="profile_popup_profile_avatar_default" 
				location-in-interfacebuilder="10.0, 10.0"
				width="48.0" height="48.0"
				phony-path="profile_popup/sprites/profile_avatar_default.png"  />
		</sprite>
	</sprites>
	<menuitems array="false" comment="create group of menuitems">
		<menuitem visible="true" comment="">
			<position-name value="close" />
			<anchorpoint value="0.5, 0.5" />
			<position value="0.0, 0.0" />
			<z-index value="0" />
			<sprites array="false" comment="create group of sprites">
				<sprite visible="true" comment="">
					<position-name value="profile_close_active" unlocated="true" />
					<image id="profile_popup_close_profile_close_active" 
						location-in-interfacebuilder="10.0, 10.0"
						width="32.0" height="32.0"
						phony-path="profile_popup/menuitems/close/sprites/profile_close_active.png"  />
				</sprite>
				<sprite visible="true" comment="">
					<position-name value="profile_close_normal" unlocated="true" />
					<image id="profile_popup_close_profile_close_normal" 
						phony-path="profile_popup/menuitems/close/sprites/profile_close_normal.png"  />
				</sprite>
			</sprites>
		</menuitem>
		<menuitem visible="true" comment="">
			<position-name value="edit" />
			<anchorpoint value="0.5, 0.5" />
			<position value="0.0, 0.0" />
			<z-index value="0" />
			<sprites array="false" comment="create group of sprites">
				<sprite visible="true" comment="">
					<position-name value="profile_edit_active" unlocated="true" />
					<image id="profile_popup_edit_profile_edit_active" 
						location-in-interfacebuilder="10.0, 10.0"
						width="60.0" height="25.0"
						phony-path="profile_popup/menuitems/edit/sprites/profile_edit_active.png"  />
				</sprite>
				<sprite visible="true" comment="">
					<position-name value="profile_edit_normal" unlocated="true" />
					<image id="profile_popup_edit_profile_edit_normal" 
						phony-path="profile_popup/menuitems/edit/sprites/profile_edit_normal.png"  />
				</sprite>
			</sprites>
		</menuitem>
	</menuitems>
</view>
